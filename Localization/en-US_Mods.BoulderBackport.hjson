Configs: {
	BackportConfig: {
		DisplayName: Backport Config

		VanillaTownNPCPortraits: {
			Label: Vanilla Town NPC Portraits
			Tooltip: When enabled, portraits for vanilla town NPCs from Terraria 1.4.5 will show up when talking to them.
		}

		ModdedTownNPCPortraits: {
			Label: Modded Town NPC Portraits
			Tooltip: When enabled, portraits for modded town NPCs will show up when talking to them.
		}

		KingSlimeBossMusic: {
			Label: King Slime Boss Music
			Tooltip: When enabled, the King Slime boss music from Terraria 1.4.5 will play during the fight.
		}

